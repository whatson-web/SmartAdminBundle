{% block javascript %}

<script>

var notif = function (type, content) {

   var title = '';
   var color = '';
   var icone = '';

    switch(type) {

        case 'success' :
            title = 'Opération réussie';
            icone = 'fa fa-thumbs-up bounce animated';
            color = '#739E73';
            break;

        case 'error' :

            title = 'Opération échouée';
            icone = 'fa fa-warning shake animated';
            color = '#C46A69';
            break;

    }


    $.smallBox({
        title : title,
        content : content,
        color   : color,
        iconSmall : icone,
        timeout : 4000
    });

}


$(document).ready(function() {

    pageSetUp();

    {% for label, flashes in app.session.flashbag.all %}

        {% for flash in flashes %}
            notif('{{ label }}', '{{ flash }}');
        {% endfor %}

    {% endfor %}


});

</script>

{{ Smart.modalSuccess() }}

{% endblock %}

